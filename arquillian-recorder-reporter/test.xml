<report xmlns="http://jboss.org/schema/arquillian/recorder/reporter" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jboss.org/schema/arquillian/recorder/reporter https://raw2.github.com/smiklosovic/arquillian-unified-recorder/reporter/arquillian-recorder-reporter/src/main/resources/xsd/report.xsd">
    <suite>
        <testClass>
            <testClassName>org.aerogear.push.testcase</testClassName>
            <testMethod>
                <testMethodName>someFakeTest</testMethodName>
                <result>passed</result>
                <duration unit="second">45</duration>
                <screenshots>
                    <screenshot>
                        <when>before</when>
                        <file>screenshots/someTest/screen.png</file>
                        <type>png</type>
                    </screenshot>
                </screenshots>
            </testMethod>
            <testMethod>
                <testMethodName>someOtherFakeTest</testMethodName>
                <result>passed</result>
                <duration unit="minute">20</duration>
                <screenshots>
                    <screenshot>
                        <when>before</when>
                        <file>screenshots/someTest/screen.png</file>
                        <type>png</type>
                    </screenshot>
                    <screenshot>
                        <when>after</when>
                        <file>screenshots/someTest/screen.png</file>
                        <type>png</type>
                    </screenshot>
                </screenshots>
            </testMethod>
        </testClass>
        <testClass>
            <testClassName>org.aerogear.push.other.testcase</testClassName>
            <testMethod>
                <testMethodName>someFakeTest2</testMethodName>
                <result>passed</result>
                <duration unit="hour">12</duration>
                <screenshots>
                    <screenshot>
                        <when>before</when>
                        <file>screenshots/someTest/screen.png</file>
                        <type>png</type>
                    </screenshot>
                    <screenshot>
                        <when>before</when>
                        <file>screenshots/someTest/screen2.png</file>
                        <type>png</type>
                    </screenshot>
                </screenshots>
            </testMethod>
            <testMethod>
                <testMethodName>someOtherFakeTest3</testMethodName>
                <result>passed</result>
                <duration>8</duration>
            </testMethod>
        </testClass>
    </suite>
</report>